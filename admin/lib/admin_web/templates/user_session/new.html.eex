<h1>Log in</h1>

<%= form_for @conn, Routes.user_session_path(@conn, :create), [as: :user], fn f -> %>
  <%= if @error_message do %>
    <div class="alert alert-danger">
      <p><%= @error_message %></p>
    </div>
  <% end %>
<% end %>

<%= link "LOGIN WITH DISCORD", to: Routes.user_oauth_path(@conn, :request, "discord") %>
